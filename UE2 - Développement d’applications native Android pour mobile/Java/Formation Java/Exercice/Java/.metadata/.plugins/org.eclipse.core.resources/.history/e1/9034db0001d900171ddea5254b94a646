package fr.Run;
import fr.dessin.Point2D.Point2D;

public class Run {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Point2D p = new Point2D();
		
		p.translate(5, 6);
		p.afficher();
		p.setX(10);
		p.afficher();
		
		Point2D p1 = new Point2D(45,8);
		System.out.println("2eme");
		p1.afficher();
		p1.translate(5, 6);
		p1.afficher();
		p1.setY(10);
		p.afficher();
		

	}

}
