package fr.exemple;

public class Chat {
		private String nom;
		private int poids;
		
	
		public Chat(){
			this.nom = nom;
			this.poids = poids;
			
			
			}


		public String getNom() {
			return nom;
		}


		public void setNom(String nom) {
			this.nom = nom;
		}


		public int getPoids() {
			return poids;
		}


		public void setPoids(int poids) {
			this.poids = poids;
		}
		
		public boolean faireQQChose(int unParam) {
			if(unParam ==0) {
				return false;
			}
			return true;
		}
		
		public static int getNbPattes() {
			return nbPattes;
		}
		
}
