package fr.banque;

public class Client {

	
		private String nom;
		private String prenom;
		private int age;
		private int numero;
		private Compte[] tabComptes;
		
				public Client() {
				this(0, null, null, 0);
			}
		

		public Client(int age, String prenom, String nom, int numero) {
			super();
			this.nom = nom;
			this.prenom = prenom;
			this.age = age;
			this.numero = numero;
		}
		
		
		
		
		
	
	
	
}
